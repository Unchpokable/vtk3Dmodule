<machine xmlns="x-schema:PowermillMachineTool" POST="pmpMultiaxis.mxs4PMill">
  <table_attach_point PART="table" X="0" Y="0" Z="0" />
  <head_attach_point  PART="head" X="0" Y="0" Z="-43" I="0" J="0" K="1" />
  <!-- Head -->
  <machine_part NAME="head">
    <model_list>
      <dmt_file>
        <!-- Comment !-->
        <path FILE="probeheads/T25G/Grey.dmt"/>
        <rgb R="192" G="192" B="192"/>
      </dmt_file>
      <dmt_file>
        <!-- Comment !-->
        <path FILE="probeheads/T25G/DarkGrey.dmt"/>
        <rgb R="100" G="100" B="100"/>
      </dmt_file>
      <dmt_file>
        <!-- Comment !-->
        <path FILE="probeheads/T25G/Black.dmt"/>
        <rgb R="5" G="5" B="5"/>
      </dmt_file>      
      <dmt_file>
        <!-- Comment !-->
        <path FILE="probeheads/T25G/Red.dmt"/>
        <rgb R="255" G="0" B="0"/>
      </dmt_file>    
    </model_list>
  </machine_part>
  <machine_part NAME="table"/>
</machine>
